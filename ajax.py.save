import flask_restful

from EVmiRNA import app, api
from EVmiRNA.core import mongo

from flask_restful import Resource, fields, marshal_with, reqparse, marshal


test_fields = {
    'fields1':fields.String,
    'fields2':fields.Interger
    'fieldsx':fields.String(attribute='fields3')
        }

class FuzzyFoo(Resource):
    @marshal_with(test_fields)
    def get(self):
        a = {'fields1':'a','fields2':1,'fields3':'A'}
        return a

api.add_resource(FuzzyFoo,'/api/test')
